{
  "headers": [
    {
      "source" : "*",
      "headers" : [
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        },
        {
          "key" : "Content-Security-Policy",
          "value" : "default-src http://localhost:8083; script-src 'unsafe-inline'"
        }
      ]
    }
  ]
}

